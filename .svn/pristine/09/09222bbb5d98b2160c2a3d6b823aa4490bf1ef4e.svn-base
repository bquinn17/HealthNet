{% extends "health/base.html" %}

{% block content %}

    <div class = "col-lg-6 col-lg-offset-3">

        <div class = "row">
            <h3>{{ message }}</h3>
        </div>

        <div class = "row">

            <table class = "table table-striped table-hover">

                <tbody>

                    {% for i in errors %}

                        <tr>
                            <td>{{ i }}</td>
                        </tr>

                    {% endfor %}

                </tbody>

            </table>

        </div>

    </div>




{% endblock %}